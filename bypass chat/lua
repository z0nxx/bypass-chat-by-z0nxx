-- –£–Ω–∏–≤–µ—Ä—Å–∞–ª—å–Ω—ã–π —Å–∫—Ä–∏–ø—Ç –¥–ª—è Roblox (—á–∞—Ç-–∏–Ω—Ç–µ—Ä—Ñ–µ–π—Å)
local player = game:GetService("Players").LocalPlayer
local textChatService = game:GetService("TextChatService")
local chatChannel = textChatService.TextChannels.RBXGeneral
local userInputService = game:GetService("UserInputService")
local runService = game:GetService("RunService")

-- –°–æ–∑–¥–∞–µ–º –∏–Ω—Ç–µ—Ä—Ñ–µ–π—Å
local screenGui = Instance.new("ScreenGui")
screenGui.Parent = player:WaitForChild("PlayerGui")

local frame = Instance.new("Frame")
frame.Size = UDim2.new(0.3, 0, 0.2, 0)
frame.Position = UDim2.new(0.35, 0, 0.4, 0)
frame.BackgroundColor3 = Color3.new(0.1, 0.1, 0.1)
frame.BackgroundTransparency = 0.3
frame.BorderSizePixel = 0
frame.Active = true
frame.Draggable = true
frame.Parent = screenGui

-- –°–º–∞–π–ª–∏–∫
local emojiLabel = Instance.new("TextLabel")
emojiLabel.Size = UDim2.new(1, 0, 0.2, 0)
emojiLabel.Position = UDim2.new(0, 0, 0, 0)
emojiLabel.BackgroundTransparency = 1
emojiLabel.Text = "ü§¨MADE BY Z0NXXü§¨"  -- –°–º–∞–π–ª–∏–∫ –∏ —Ç–µ–∫—Å—Ç
emojiLabel.TextColor3 = Color3.new(1, 1, 1)
emojiLabel.TextScaled = true
emojiLabel.TextSize = 30
emojiLabel.Font = Enum.Font.SourceSansBold
emojiLabel.Parent = frame

-- –¢–µ–∫—Å—Ç–æ–≤–æ–µ –ø–æ–ª–µ
local textBox = Instance.new("TextBox")
textBox.Size = UDim2.new(0.9, 0, 0.4, 0)
textBox.Position = UDim2.new(0.05, 0, 0.25, 0)
textBox.BackgroundColor3 = Color3.new(1, 1, 1)
textBox.BackgroundTransparency = 0.5
textBox.TextColor3 = Color3.new(0, 0, 0)
textBox.PlaceholderText = "–í–≤–µ–¥–∏—Ç–µ —Å–æ–æ–±—â–µ–Ω–∏–µ..."
textBox.TextScaled = true
textBox.Parent = frame

-- –ö–Ω–æ–ø–∫–∞ "–û—Ç–ø—Ä–∞–≤–∏—Ç—å"
local sendButton = Instance.new("TextButton")
sendButton.Size = UDim2.new(0.4, 0, 0.2, 0)
sendButton.Position = UDim2.new(0.3, 0, 0.7, 0)
sendButton.BackgroundColor3 = Color3.new(0.1, 0.5, 1)
sendButton.BackgroundTransparency = 0.3
sendButton.Text = "–û—Ç–ø—Ä–∞–≤–∏—Ç—å"
sendButton.TextColor3 = Color3.new(1, 1, 1)
sendButton.TextScaled = true
sendButton.Parent = frame

-- –ù–∞–¥–ø–∏—Å—å "made by z0nxx"
local madeByLabel = Instance.new("TextLabel")
madeByLabel.Size = UDim2.new(0.9, 0, 0.1, 0)
madeByLabel.Position = UDim2.new(0.05, 0, 0.9, 0)
madeByLabel.BackgroundTransparency = 1
madeByLabel.Text = "made by z0nxx"
madeByLabel.TextColor3 = Color3.new(1, 1, 1)
madeByLabel.TextXAlignment = Enum.TextXAlignment.Center
madeByLabel.Font = Enum.Font.SourceSansBold
madeByLabel.TextSize = 14
madeByLabel.Parent = frame

-- –ê–Ω–∏–º–∞—Ü–∏—è –ø–µ—Ä–µ–ª–∏–≤–∞—é—â–µ–≥–æ—Å—è —Ç–µ–∫—Å—Ç–∞
local function colorAnimation()
    local time = 0
    while true do
        time = time + 0.01
        -- –ü–ª–∞–≤–Ω–æ–µ –∏–∑–º–µ–Ω–µ–Ω–∏–µ –æ—Ç —á–µ—Ä–Ω–æ–≥–æ (0, 0, 0) –¥–æ –±–µ–ª–æ–≥–æ (1, 1, 1)
        local colorValue = (math.sin(time) + 1) / 2
        emojiLabel.TextColor3 = Color3.new(colorValue, colorValue, colorValue)
        madeByLabel.TextColor3 = Color3.new(colorValue, colorValue, colorValue)
        runService.RenderStepped:Wait()
    end
end

-- –ó–∞–ø—É—Å–∫ –∞–Ω–∏–º–∞—Ü–∏–∏
task.spawn(colorAnimation)

-- –§—É–Ω–∫—Ü–∏—è –¥–ª—è –æ—Ç–ø—Ä–∞–≤–∫–∏ —Å–æ–æ–±—â–µ–Ω–∏—è
local function sendMessage()
    local message = textBox.Text
    if message and message ~= "" then
        -- –î–æ–±–∞–≤–ª—è–µ–º –ø—Ä–∏–ø–∏—Å–∫—É
        local newMessage = message .. " "
        
        -- –û—Ç–ø—Ä–∞–≤–ª—è–µ–º —Å–æ–æ–±—â–µ–Ω–∏–µ –≤ —á–∞—Ç
        chatChannel:SendAsync(newMessage)
        
        -- –û—á–∏—â–∞–µ–º —Ç–µ–∫—Å—Ç–æ–≤–æ–µ –ø–æ–ª–µ
        textBox.Text = ""
    end
end

-- –û–±—Ä–∞–±–æ—Ç–∫–∞ –Ω–∞–∂–∞—Ç–∏—è –∫–Ω–æ–ø–∫–∏ "–û—Ç–ø—Ä–∞–≤–∏—Ç—å"
sendButton.MouseButton1Click:Connect(sendMessage)

-- –û–±—Ä–∞–±–æ—Ç–∫–∞ –Ω–∞–∂–∞—Ç–∏—è Enter –≤ —Ç–µ–∫—Å—Ç–æ–≤–æ–º –ø–æ–ª–µ
textBox.FocusLost:Connect(function(enterPressed)
    if enterPressed then
        sendMessage()
    end
end)
